class Animal{

    init(kind, legs, sound) {
        this <- kind = kind;
        this <- legs = legs;
        this <- sound = sound;
    }

    make_sound() {
        return this -> sound;
    }

    me() {
        return this;
    }

    static multiply(n) {
        return n * n;
    }

}

let cat = Animal("cat", 4, "meow");
let size = _M_size_of(cat);

println cat -> make_sound();
println size;

