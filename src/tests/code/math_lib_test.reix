expose class MathTest {

    static run() {

        println "Running Math Tests...";

        println "PI: " + Math.PI();
        println "E: " + Math.E();

        println "pow(10 ,10): " + Math.pow(10, 10);
        println "sqrt(9): " + Math.sqrt(9);
        println "cbrt(27): " + Math.cbrt(27);
        println "abs(-5): " + Math.abs(-5);
        println "sign(-10): " + Math.sign(-10);
        println "sign(0): " + Math.sign(0);
        println "sign(42): " + Math.sign(42);

        println "floor(3.7): " + Math.floor(3.7);
        println "ceil(3.2): " + Math.ceil(3.2);
        println "round(3.5): " + Math.round(3.5);
        println "round(3.2): " + Math.round(3.2);
        println "trunc(3.9): " + Math.trunc(3.9);

        println "clamp(10, 0, 5): " + Math.clamp(10, 0, 5);
        println "clamp(-1, 0, 5): " + Math.clamp(-1, 0, 5);
        println "clamp(3, 0, 5): " + Math.clamp(3, 0, 5);

        println "sin(PI/2): " + Math.sin(Math.PI() / 2);
        println "tan(PI/4): " + Math.tan(Math.PI() / 4);
        println "asin(1): " + Math.asin(1);
        println "acos(0): " + Math.acos(0);
        println "atan(1): " + Math.atan(1);

        let deg = 90;
        let rad = Math.to_radians(deg);
        println "to_radians(90): " + rad;
        println "to_degrees(PI/2): " + Math.to_degrees(Math.PI() / 2);

        println "log(E): " + Math.log(Math.E());
        println "log10(100): " + Math.log10(100);

        let rand = Math.random();
        println "random(): " + rand;

        let rand_range = Math.random_range(1, 10);
        println "random_range(1,10): " + rand_range;

    }

}

