let app = _NET_router();

fn home(a) {
    let b = _NET_get("http://api.weatherapi.com/v1/forecast.json?key=aec71c1efde74d9c9f1132807252308&q=Mandalay&days=3&aqi=no&alerts=no");
    return b;
}

fn time() {
    return _C_format_time();
}

_NET_router_get(app, "/", home);

_NET_router_get(app, "/time", time);

println("Starting server at http://127.0.0.1:8080");
_NET_serve(app, "127.0.0.1:8080");
