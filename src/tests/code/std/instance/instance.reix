expose class Instance {

    // Get the type of obj
    typeof(type) {

        if (@typeof(this, type)) return true;
        return false;

    }

    // Destroy an attribute
    destroy(attribute) {
        @destroy(attribute);
    }

    // Saftly mutate an attribute of obj
    mutate(attribute, new_val) {
        @mutate(attribute, new_val);
    }

}
